<html>
  <style>
  th {
    padding-top: 2px;
    padding-bottom: 2px;
    text-align: center;
    background-color: #4CAF50;
    color: white;
  }
  td,th {
    border: 1px solid #ddd;
    padding: 4px;
    text-align: center;
  }
  tr:nth-child(even){background-color: #f2f2f2;}
  tr:hover {background-color: #ddd;}
  </style>
  <body>
    <!-- date time picker -->
    <form action="/datalog">
      <label for="from">From:</label>
      <input type="date" id="from" name="fromdate">
      <input type="time" id="from" name="fromtime">
      <label for="to">To:</label>
      <input type="date" id="to" name="todate">
      <input type="time" id="from" name="totime">
      <input type="submit" value="Submit">
    </form>
    <!-- ... -->
    <table>
       <tr>
        <th width="50px">STT</th>
        <th>Itemcode</th>
        <th>RAW</th>
      </tr>
      <% if(raw.length){ 
        for(var i = 0;i < raw.length;i++) { %>
      <tr>
        <td><%=(i+1)%></td>
        <td><%=raw[i].barcode%></td>
        <td><%=raw[i].rawScale%><%=raw[i].rawU81%></td>
      </tr>
      <% }
      }else{ %>
      <tr>
        <td colspan="3">No data</td>
      </tr>
    <% } %>
    </table>
    </body></html>